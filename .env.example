# Environment variables for Beltways RTIIS
# Use this connection string for Supabase Postgres in production deployments
DATABASE_URL=postgresql+psycopg2://YOUR_SUPABASE_USER:YOUR_PASSWORD@YOUR_SUPABASE_HOST:5432/postgres

# Optional local SQLite fallback (uncomment to use locally)
# DATABASE_URL=sqlite:///./rtis.db
OPENAI_API_KEY=your_openai_api_key_here
LLM_PROVIDER=openai
BACKEND_PORT=8000
FRONTEND_PORT=3000
